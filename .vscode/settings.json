// Placez vos paramètres dans ce fichier pour remplacer les paramètres par défaut
{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.scrollback": 10000,
  "workbench.iconTheme": "vscode-icons",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "prettier.singleQuote": true,
  "prettier.printWidth": 140,
  "prettier.trailingComma": "none",
  "prettier.prettierPath": "./node_modules/prettier",
  "editor.fontSize": 14,
  "editor.accessibilityPageSize": 700,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/out": true
  },
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html"],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.addMissingImports": "explicit"
  },
  "workbench.colorTheme": "GitHub Dark Default",
  "diffEditor.ignoreTrimWhitespace": true,
  "cSpell.language": "fr, en",
  "cSpell.dictionaries": ["fr", "en"],
  "cSpell.enabledFileTypes": {
    "typescript": true,
    "html": true,
    "json": true,
    "markdown": true
  },
  "cSpell.words": ["codejinn", "jsoneditor", "logocodejinn", "openai", "tuello"]
}
