{
  "name": "tuello",
  "version": "1.2.8.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "patch-package",
    "build": "npm run clean && ng build --configuration development && npm run _build:tuello:chrome",
    "build:prod": "npm run clean && ng build && npm run _build:tuello:chrome:prod",
    "_build:tuello:chrome": "webpack --config ./chrome/webpack.config.js",
    "_build:tuello:chrome:prod": "webpack --config ./chrome/webpack.config.prod.js",
    "build:stats:angular": "ng build --configuration=production --source-map",
    "build:stats:chrome": "webpack --config ./chrome/webpack.config.prod.js --profile --json > ./dist/tuello/stats-chrome.json",
    "analyze:angular": "npx source-map-explorer dist/tuello/**/*.js",
    "analyze:chrome": "webpack-bundle-analyzer dist/tuello/stats-chrome.json",
    "clean": "rimraf dist tmp",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@angular/router": "^21.0.6",
    "@ngbracket/ngx-layout": "^21.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@types/component-emitter": "^1.2.14",
    "animate.css": "^4.1.1",
    "big-json-viewer": "^0.2.2",
    "bootstrap": "^5.3.8",
    "buffer": "^6.0.3",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.5",
    "json-find": "^2.0.8",
    "json5": "^2.2.3",
    "lz-string": "^1.5.0",
    "ngx-json-viewer": "^3.2.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "rxjs": "^7.8.2",
    "rxjs-pausable": "^1.0.0",
    "simptip": "^1.0.4",
    "tslib": "^2.8.1",
    "vanilla-jsoneditor": "^3.11.0",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular-eslint/builder": "^21.1.0",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/schematics": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@types/chrome": "^0.0.293",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.13",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^22.10.5",
    "@types/selenium-webdriver": "^4.35.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^8.57.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "source-map-explorer": "^2.5.3",
    "ts-loader": "^9.5.4",
    "typescript": "~5.9.3",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
