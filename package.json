{
  "name": "tuello",
  "version": "1.1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "patch-package",
    "build": "npm run clean && ng build --configuration development && npm run _build:tuello:chrome",
    "build:prod": "npm run clean && ng build && npm run _build:tuello:chrome:prod",
    "_build:tuello:chrome": "webpack --config ./chrome/webpack.config.js",
    "_build:tuello:chrome:prod": "webpack --config ./chrome/webpack.config.prod.js",
    "build:stats:angular": "ng build --configuration=production --source-map",
    "build:stats:chrome": "webpack --config ./chrome/webpack.config.prod.js --profile --json > ./dist/tuello/stats-chrome.json",
    "analyze:angular": "npx source-map-explorer dist/tuello/main.*.js",
    "analyze:chrome": "webpack-bundle-analyzer dist/tuello/stats-chrome.json",
    "clean": "rimraf dist tmp",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.4",
    "@angular/cdk": "^19.1.2",
    "@angular/common": "^19.1.4",
    "@angular/compiler": "^19.1.4",
    "@angular/core": "^19.1.4",
    "@angular/forms": "^19.1.4",
    "@angular/material": "^19.1.2",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "^19.1.4",
    "@angular/router": "^19.1.4",
    "@ngbracket/ngx-layout": "^19.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@types/component-emitter": "^1.2.14",
    "animate.css": "^4.1.1",
    "big-json-viewer": "^0.1.7",
    "bootstrap": "^5.3.3",
    "buffer": "^6.0.3",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.5",
    "json-find": "^2.0.8",
    "json5": "^2.2.3",
    "jsoneditor": "^10.1.2",
    "ngx-json-viewer": "^3.2.1",
    "pixelmatch": "^6.0.0",
    "pngjs": "^7.0.0",
    "rxjs": "^7.8.1",
    "rxjs-pausable": "^1.0.0",
    "simptip": "^1.0.4",
    "tslib": "^2.3.0",
    "vanilla-jsoneditor": "^2.4.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-devkit/build-angular": "^19.1.5",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "^19.1.5",
    "@angular/compiler-cli": "^19.1.4",
    "@types/chrome": "^0.0.293",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^22.10.5",
    "@types/selenium-webdriver": "^4.1.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.57.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "source-map-explorer": "^2.5.3",
    "ts-loader": "^9.2.8",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.7.3",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts\"": [
      "prettier --write",
      "tslint --project tsconfig.json",
      "git add"
    ]
  }
}
